{
    "name": "lathanhvien/custom-belongs-to-many-field",
    "description": "belongsToMany nova representation in field.",
    "keywords": [
        "laravel",
        "nova",
        "belongsToMany",
        "Many to Many Field"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Pifpif\\CustomBelongsToManyField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pifpif\\CustomBelongsToManyField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
